<UserControl x:Class="Jump01.UCPersonSeparation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Jump01"
             mc:Ignorable="d" 
             d:DesignHeight="6" d:DesignWidth="36">
    <Grid Margin="-36,-90,0,0">
        <Canvas Width="72" Height="96" >
            <Canvas>
                <Image x:Name="head" Source="Images/Head.png" Width="23" Height="23" Canvas.Left="25" Canvas.Top="4"/>
            </Canvas>
            
            <Canvas>
                <Image x:Name="body" Source="Images/Body.png" Height="65" Width="50" Canvas.Left="11" Canvas.Top="30" RenderTransformOrigin="0.5,1" />
            </Canvas>
            <!--<Rectangle Fill="Yellow" Width="36" Height="6" Canvas.Left="36" Canvas.Top="90" ></Rectangle>-->
        </Canvas>
    </Grid>
</UserControl>
