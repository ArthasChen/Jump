<UserControl x:Class="Jump01.UCBackgroundxaml"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Jump01"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="450">
    <Grid>
        <Canvas x:Name="Layer_1" Canvas.Left="0" Canvas.Top="0">
            <Image x:Name="Image" Source="Images/background.png" Width="450" Height="800" Canvas.Left="0" Canvas.Top="0">
                <Image.RenderTransform>
                    <TransformGroup>
                        <MatrixTransform Matrix="1,0,0,1,0,4.06901e-005"/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
        </Canvas>

    </Grid>
</UserControl>
